var url = "http://api.wunderground.com/api/4c3684666a48c28f/geolookup/forecast/q/FL/Coconut_Creek.json";



var error = function(){
	alert("Oops, something went wrong!");
};


var client = Ti.Network.createHTTPClient({
	onload: app.success,
	onerror: error,
	timeout: 5000
});

var app = require("app");

client.open("GET", url);

client.send();
