var mainWin = Ti.UI.createWindow({
	title: "Welcome!",
	backgroundColor: "#EEE"
});

var navWindow = Ti.UI.iOS.createNavigationWindow({
	window: mainWin
});

var forecast = Ti.UI.createLabel({
	text: "Click for todays forecast!",
	textAlign: "center",
	font: {fontSize: "20", fontWeight: "bold"},
	borderColor: "#000",
	borderWidth: 3,
	backgroundColor: "#EEE",
});

var getData = require("data");

navWindow.add(mainWin, forecast);
navWindow.open();
