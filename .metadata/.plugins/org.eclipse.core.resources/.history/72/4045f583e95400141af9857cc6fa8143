var window = Ti.UI.createWindow({ 
	backgroundColor: "#fff" 
});
var imageContainer = Ti.UI.createView();

var getImage = function(){
	if(Ti.Network.online){
		var xhr = Ti.Network.createHTTPClient({
			onload: function(){
				var response = JSON.parse(this.responseText);
				for( var i = 0 ; i < response.forecast.forecastday.length ; i++ ) {
					var image = Ti.UI.createImageView({
						image : response.forecast.forecastday[i].icon_url
					}
					,imageContainer.add(image);
				}
			}
			onerror : function {},
			timeout: 5000
		});
		xhr.open("GET", "http://api.wunderground.com/api/440c8cedbd410d02/conditions/q/FL/Coconut_Creek.json");
		xhr.send();
	}
else { alert(“Error” + this.responseText.error); }
};

window.add(imageContainer);