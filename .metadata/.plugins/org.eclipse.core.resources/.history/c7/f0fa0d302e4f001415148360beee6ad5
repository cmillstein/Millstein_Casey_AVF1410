var id = ""
var long = Titanium.App.Properties.getString("longitude");
var lat = Titanium.App.Properties.getString("latitude");



var Cloud = require('ti.cloud');
Cloud.debug = true;
var loginUser = function(){
	Cloud.Users.login({
		login: 'admin',
		password: 'admin'
	}, function(e){
		if (e.success){
			var user = e.users[0];
			Ti.API.info('Success!\n' + 
				'ACS User ID: ' + user.id + '\n' + 
				'ACS App sessionId: ' + Cloud.sessionId + '\n' + 
				'ACS App Username: ' + user.username);
		} else {
			alert((e.error && e.message) || JSON.stringify(e));
		}
	});
}; 
loginUser();
